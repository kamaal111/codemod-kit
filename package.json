{
  "name": "codemod-kit",
  "version": "0.0.1",
  "type": "module",
  "author": "Kamaal Farah",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "build:clean": "rm -rf dist tsconfig.tsbuildinfo && pnpm run build",
    "dev": "rslib build --watch",
    "format": "prettier --write .",
    "format:check": "prettier . --check",
    "lint": "eslint .",
    "prepare": "husky",
    "test": "vitest run",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@kamaalio/prettier-config": "^0.1.2",
    "@rslib/core": "^0.10.2",
    "@types/node": "^22.15.32",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "prettier": "@kamaalio/prettier-config"
}
