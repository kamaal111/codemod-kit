{
  "name": "@kamaalio/codemod-kit",
  "version": "0.0.1",
  "type": "module",
  "author": "Kamaal Farah",
  "license": "MIT",
  "repository": "kamaal111/codemod-kit",
  "bugs": "https://github.com/kamaal111/codemod-kit/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "build:clean": "rm -rf dist tsconfig.tsbuildinfo && pnpm run build",
    "dev": "rslib build --watch",
    "format": "prettier --write .",
    "format:check": "prettier . --check",
    "lint": "eslint .",
    "prepare": "husky",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ast-grep/napi": "^0.40.5",
    "@kamaalio/kamaal": "^0.8.0",
    "execa": "^9.6.1",
    "fast-glob": "^3.3.3",
    "neverthrow": "^8.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@kamaalio/prettier-config": "^0.1.2",
    "@rslib/core": "^0.19.1",
    "@types/node": "^25.0.6",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "prettier": "@kamaalio/prettier-config",
  "packageManager": "pnpm@10.28.0"
}
